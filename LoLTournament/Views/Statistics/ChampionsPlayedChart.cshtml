@model LoLTournament.Models.ChampionsPlayedViewModel

@{
    var myChart = new Chart(width: 800, height: 800)
        .AddTitle("Most Played Champions")
        .AddSeries(
            name: "champions",
            xValue: Model.ChampionsPlayed.Keys,
            yValues: Model.ChampionsPlayed.Values)
        .Write();
}