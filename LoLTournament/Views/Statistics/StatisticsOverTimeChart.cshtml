@model LoLTournament.Models.StatisticsOverTimeViewModel

@{ 
    var myChart = new Chart(width: 800, height: 800)
        .AddTitle("Kills over time")
        .AddSeries(chartType: "Line",
            name: "Total Kills",
            xValue: Model.Kills.Keys,
            yValues: Model.Kills.Values)
        .AddSeries(chartType: "Line",
            name: "Total Assists",
            xValue: Model.Assists.Keys,
            yValues: Model.Assists.Values)
            .AddLegend()
        .Write();
}