@{
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}
@model LoLTournament.Models.Match

<h1>Set match duration</h1>

<p>
    What's the match duration?
</p>

<form method="get">
    <input type="text" name="confirmation" value="00:20:00"/>
    <input type="submit" name="submit" value="Submit" class="btn btn-primary"/>
    <input type="hidden" name="matchId" value="@Model.Id.ToString()"/>
</form>
